{
  "name": "@openastrodynamics/r3f",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MPL-2.0",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --loader \".jpg=dataurl\" --loader \".hdr=dataurl\"",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react --loader \".jpg=dataurl\" --loader \".hdr=dataurl\"",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@openastrodynamics/tsconfig": "*",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/three": "^0.143.0",
    "eslint": "^8.21.0",
    "eslint-config-openastrodynamics": "*",
    "react": "^18.2.0",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openastrodynamics/core": "*",
    "@react-three/drei": "^9.19.6",
    "@react-three/fiber": "^8.3.0",
    "three": "^0.143.0",
    "zustand": "^4.0.0"
  }
}
